{
    "name": "cosmic-shapes",
    "displayName": "Cosmic Shapes",
    "version": "1.0.0",
    "description": "A simple visualization app for ipad",
    "main": "index.js",
    "scripts": {
        "ios-dev": "cordova serve ios --target=\"iPad--5th-generation-, 11.2\"",
        "browser-dev": "cordova serve browser",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Robert Richardson",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-browser": "^5.0.3",
        "cordova-ios": "4.5.4",
        "cordova-plugin-whitelist": "^1.3.3"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "browser",
            "ios"
        ]
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "trailingComma": "es5"
    }
}