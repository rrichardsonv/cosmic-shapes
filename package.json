{
  "name": "cosmic-shapes",
  "displayName": "Cosmic Shapes",
  "version": "1.0.0",
  "description": "A simple visualization app for ipad",
  "main": "index.js",
  "scripts": {
    "browser-dev": "cordova run browser",
    "build": "parcel build src/index.js --out-dir www/js --out-file index.js",
    "ios-dev": "cordova run ios --target=\"iPad--5th-generation-, 11.2\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Robert Richardson",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova": "^8.0.0",
    "cordova-browser": "^5.0.3",
    "cordova-ios": "4.5.4",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "ios"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.0",
    "parcel": "^1.9.6"
  }
}
